preplace inst axi_bram_ctrl_0 -pg 1 -lvl 5 -y 440 -defaultsOSRD
preplace inst processing_system7_0_axi_periph -pg 1 -lvl 2 -y 410 -defaultsOSRD
preplace inst processing_system7_0 -pg 1 -lvl 5 -y 140 -defaultsOSRD
preplace inst axi_bram_ctrl_1 -pg 1 -lvl 3 -y 550 -defaultsOSRD
preplace netloc processing_system7_0_DDR 1 5 2 NJ 100 NJ
preplace netloc axi_mem_intercon_M01_AXI 1 4 1 N
